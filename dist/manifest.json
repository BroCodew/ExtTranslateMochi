{
  "name": "Extension App",
  "description": "",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open Extension App"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content_script.jsx-loader-fded4f6b.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "assets/content_script-f03861ba.css",
        "assets/index-49f6d87a.css"
      ]
    }
  ],
  "host_permissions": [
    "*://*.facebook.com/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "commands": {
    "Ctrl+B": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      },
      "description": "Reload Extension"
    }
  },
  "options_page": "options.html",
  "permissions": [
    "background",
    "contextMenus",
    "bookmarks",
    "tabs",
    "storage",
    "history",
    "cookies",
    "scripting",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://*/*"
      ],
      "resources": [
        "css/*"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/Icon_Translate-1e62bf37.png",
        "assets/client-3c8a2c3a.js",
        "assets/content_script.jsx-5f77e926.js"
      ],
      "use_dynamic_url": true
    }
  ]
}
